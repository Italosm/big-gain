{
  "name": "big-gain",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "dev": "npx dotenv-cli -e .env.development -- tsx watch src/shared/infrastructure/http/server.ts",
    "prisma:generate": "npx dotenv-cli -e .env.development -- prisma generate --schema=src/shared/infrastructure/database/prisma/schema.prisma",
    "prisma:migrate": "npx dotenv-cli -e .env.development -- prisma migrate dev --schema=src/shared/infrastructure/database/prisma/schema.prisma",
    "prisma:migrate-deploy": "npx dotenv-cli -e .env -- prisma migrate deploy --schema=src/shared/infrastructure/database/prisma/schema.prisma",
    "prisma:studio": "prisma studio --schema=src/shared/infrastructure/database/prisma/schema.prisma",
    "build": "prisma generate --schema=src/shared/infrastructure/database/prisma/schema.prisma && babel src --extensions \".js,.ts\" --out-dir dist --copy-files",
    "start": "NODE_TLS_REJECT_UNAUTHORIZED=0 node dist/shared/infrastructure/http/server.js",
    "start:dev": "npx dotenv-cli -e .env.development -- node dist/shared/infrastructure/http/server.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@babel/cli": "^7.24.8",
    "@babel/core": "^7.25.2",
    "@babel/node": "^7.25.0",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-decorators": "^7.24.7",
    "@babel/preset-env": "^7.25.3",
    "@babel/preset-typescript": "^7.24.7",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/express-session": "^1.18.0",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^20.14.10",
    "@typescript-eslint/eslint-plugin": "^7.16.0",
    "@typescript-eslint/parser": "^7.16.0",
    "babel-plugin-module-resolver": "^5.0.2",
    "babel-plugin-transform-typescript-metadata": "^0.3.2",
    "dotenv-cli": "^7.4.2",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "prettier": "^3.3.2",
    "prisma": "^5.16.2",
    "tsx": "^4.16.2",
    "typescript": "^5.5.3"
  },
  "dependencies": {
    "@prisma/client": "^5.16.2",
    "cors": "^2.8.5",
    "express": "^4.19.2",
    "express-async-errors": "^3.1.1",
    "express-openid-connect": "^2.17.1",
    "express-session": "^1.18.0",
    "jsonwebtoken": "^9.0.2",
    "stripe": "^16.7.0",
    "telegraf": "^4.16.3",
    "zod": "^3.23.8"
  }
}
